<p id="notice"><%= notice %></p>

<h1>Car Rental Contracts</h1>

<table>
  <thead>
    <tr>
      <th>Rdate</th>
      <th>Rcontent</th>
      <th>Rvalue</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_rental_contracts.each do |car_rental_contract| %>
      <tr>
        <td><%= car_rental_contract.rDate %></td>
        <td><%= car_rental_contract.rContent %></td>
        <td><%= car_rental_contract.rValue %></td>
        <td><%= link_to 'Show', car_rental_contract %></td>
        <td><%= link_to 'Edit', edit_car_rental_contract_path(car_rental_contract) %></td>
        <td><%= link_to 'Destroy', car_rental_contract, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car Rental Contract', new_car_rental_contract_path %>
