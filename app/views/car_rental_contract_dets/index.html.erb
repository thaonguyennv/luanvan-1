<p id="notice"><%= notice %></p>

<h1>Car Rental Contract Dets</h1>

<table>
  <thead>
    <tr>
      <th>Rdetdepday</th>
      <th>Rdetnoday</th>
      <th>Rdetcarprice</th>
      <th>Rdetdriverprice</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_rental_contract_dets.each do |car_rental_contract_det| %>
      <tr>
        <td><%= car_rental_contract_det.rdetDepDay %></td>
        <td><%= car_rental_contract_det.rdetNoDay %></td>
        <td><%= car_rental_contract_det.rdetCarPrice %></td>
        <td><%= car_rental_contract_det.rdetDriverPrice %></td>
        <td><%= link_to 'Show', car_rental_contract_det %></td>
        <td><%= link_to 'Edit', edit_car_rental_contract_det_path(car_rental_contract_det) %></td>
        <td><%= link_to 'Destroy', car_rental_contract_det, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

