<p id="notice"><%= notice %></p>

<h1>Members</h1>

<table>
  <thead>
    <tr>
      <th>Mname</th>
      <th>Msex</th>
      <th>Mdob</th>
      <th>Midno</th>
      <th>Mtel</th>
      <th>Maddress</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.mName %></td>
        <td><%= member.mSex %></td>
        <td><%= member.mDob %></td>
        <td><%= member.mIdNo %></td>
        <td><%= member.mTel %></td>
        <td><%= member.mAddress %></td>
        <td><%= link_to 'Show', member %></td>
        <td><%= link_to 'Edit', edit_member_path(member) %></td>
        <td><%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member', new_member_path %>
